{
  "$schema": "https://raw.githubusercontent.com/sketch-hq/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "commands": [
    {
      "name": "Rename layers",
      "identifier": "{{ slug }}.my-command-identifier",
      "script": "./renameLayers.js"
    },
    {
      "name": "Get imported symbol names",
      "identifier": "{{ slug }}.get-imported-symbol-names-identifier",
      "script": "./getSymbols.js"
    },
    {
      "name": "Get symbol master names",
      "identifier": "{{ slug }}.get-symbol-master-names-identifier",
      "script": "./getSymbolArtboards.js"
    }
  ],
  "menu": {
    "title": "cs-system-handoff",
    "items": [
      "{{ slug }}.my-command-identifier",
      "{{ slug }}.get-imported-symbol-names-identifie",
      "{{ slug }}.get-symbol-master-names-identifier"
    ]
  }
}
