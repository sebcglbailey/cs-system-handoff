{
  "name": "cs-system-handoff",
  "description": "",
  "version": "1.0.1",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "cs-system-handoff",
    "manifest": "src/manifest.json",
    "main": "cs-system-handoff.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build:major": "skpm-build && npm version major",
    "build:minor": "skpm-build && npm version minor",
    "build:patch": "skpm-build && npm version patch",
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Seb Bailey <sebastian@clearscore.com>",
  "repository": "https://github.com/sebcglbailey/cs-system-handoff"
}
